void setup() {
  pinMode(13, OUTPUT);     // configuro il pin 13 lato destro, come output 







}

void loop() {
  digitalWrite(13, HIGH);   // accendo il foro/piedino numero 13
  delay(2000);              // rimani in pausa per 2 secondi (2000) millisecondi
  digitalWrite(13, LOW);    // spegni il foro/piedino numero 13
  delay(5000);              // rimani in pausa per 5 secondi




}
